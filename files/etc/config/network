
config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals 'globals'
	option ula_prefix 'fd6d:7259:a206::/48'

config interface 'lan'
	option type 'bridge'
	option ifname 'eth1'
	option proto 'static'
	option netmask '255.255.255.0'
	option ipaddr '10.0.0.1'
	option delegate '0'

config device 'lan_eth1_dev'
	option name 'eth1'
	option macaddr '82:34:28:34:37:cb'

config interface 'wan'
	option ifname 'eth0'
	option _orig_ifname 'eth0'
	option _orig_bridge 'false'
	option proto 'pppoe'
	option username 'szcsdvin@163.gd'
	option password '88888888'
	option keepalive '0'
	option delegate '0'
	option ipv6 '0'

config device 'wan_eth0_dev'
	option name 'eth0'
	option macaddr '80:34:28:34:37:cb'

config interface 'docker'
	option ifname 'docker0'
	option proto 'none'
	option delegate '0'

config device
	option type 'bridge'
	option name 'docker0'

config interface 'Modem'
	option proto 'dhcp'
	option ifname 'eth0'
	option delegate '0'
	option auto '0'
	option metric '99'
	option defaultroute '0'

